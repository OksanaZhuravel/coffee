{
	"version": 2,
	"builds": [
		{
			"src": "api/**/*.ts",
			"use": "@vercel/node"
		}
	],
	"functions": {
		"api/**/*.ts": {
			"includeFiles": "api/**/*",
			"runtime": "@vercel/node"
		}
	},
	"routes": [{ "src": "/api/(.*)", "dest": "api/$1" }],
	"env": {
		"DATABASE_URL": "POSTGRES_PRISMA_URL"
	}
}
